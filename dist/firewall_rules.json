{
  "firewall_rules": [
    {
      "source": "dev_user",
      "target": "server_space",
      "port": "22",
      "protocol": "TCP",
      "purpose": "Allow SSH access for development activities"
    },
    {
      "source": "dev_user",
      "target": "mongo_atlas",
      "port": "27017",
      "protocol": "TCP",
      "purpose": "Allow MongoDB connections for data access"
    },
    {
      "source": "dev_user",
      "target": "confluent_cloud",
      "port": "9092",
      "protocol": "TCP",
      "purpose": "Allow Kafka connections for message publishing and consuming"
    },
    {
      "source": "centera_ldap_1",
      "target": "centera_ldap_2",
      "port": "389",
      "protocol": "TCP",
      "purpose": "Allow LDAP communication between Centera LDAP servers"
    },
    {
      "source": "centera_ldap_1",
      "target": "centera_ldap_3",
      "port": "389",
      "protocol": "TCP",
      "purpose": "Allow LDAP communication between Centera LDAP servers"
    },
    {
      "source": "centera_ldap_2",
      "target": "centera_ldap_3",
      "port": "389",
      "protocol": "TCP",
      "purpose": "Allow LDAP communication between Centera LDAP servers"
    }
  ]
}